#include <iostream>
#include <ctime>
using namespace std;
int main()
{
    int age;
    cout<<"Enter age: ";
    cin>>age;
    if(age<=12){
        cout<<"CHILD";
    }
    else if(age>=13 && age<18){
        cout<<"TEENAGER";
    }
    else if(age>=12 && age<60){
        cout<<"ADULT";
    }
    else{
        cout<<"SENIOR CITIZEN";
    }



}
